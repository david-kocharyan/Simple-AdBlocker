{
    "manifest_version": 2,
    "name": "Custom AdBlocker",
    "description" : "Delete All Ads!",
    "version": "0.0.1",
    "author": "David Kocharyan",
    "browser_action": {
        "default_title": "Delete Ads",
        "default_icon": "logo.png",
        "default_popup": "index.html"
    },
    "permissions":["webRequest", "webRequestBlocking", "<all_urls>"],
    "background":{
        "scripts":["content.js"]
    }
}